table {border-collapse:collapse;}

.inner {border:none;}

table td {
border: solid 1px black;
}


table {
font-family:arial, helvetica;
font-size:14pt;
background-color: #C6FCFE;
}

td {
font-size:20;
width: 140px;
padding-left: 5px;
padding-bottom: 10px;
padding-top:2px;
padding-right: 10px;
vertical-align:top;
}


.summer {background-color: #F7F67F;}
th {
font-size: 20;
font-family:arial, helvetica;
}

.dayheader {
font-size:20;
padding-bottom: 10px;
background-color: #eeeeff;
}

.srss {
width:0px;
font-size: 8pt;
padding-right: 0pt;
text-align:right;
font-family: times;
border:none;
}

.invisible {
position:absolute;
visibility:hidden;
font-size:10pt;
background:#EAF5AF;
}
.floattable .ftd {
border:solid 1px black;
font-size:10pt;
background:#1AF5AF;
padding-left:2px;
padding-right:2px;
padding-top:2px;
padding-bottom:2px;
width:0px;
}
